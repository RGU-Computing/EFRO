    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAllocatedAmount -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAllocatedAmount">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasFundingValue"/>
