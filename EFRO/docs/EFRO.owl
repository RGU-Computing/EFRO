    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasDate">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Activity"/>
