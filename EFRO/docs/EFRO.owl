        <rdfs:label>ApprenticeshipFunding</rdfs:label>
        <skos:example>Funding caps and bands specific to apprenticeship standards and frameworks.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ApprenticeshipProgramme -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ApprenticeshipProgramme">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
        <terms:description>A study program that combines on-the-job training with classroom learning, leading to recognized apprenticeship qualifications.</terms:description>
        <rdfs:label>ApprenticeshipProgramme</rdfs:label>
        <skos:example>Apprenticeships in digital marketing or construction.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ApprenticeshipProgression -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ApprenticeshipProgression">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionType"/>
        <terms:description xml:lang="en">Represents a student&apos;s transition from a study programme to an apprenticeship scheme.</terms:description>
        <rdfs:label>ApprenticeshipProgression</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ApprenticeshipProvider -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ApprenticeshipProvider">
