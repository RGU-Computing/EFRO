<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.w3id.org/EFRO/EFRO#"
     xml:base="http://www.w3id.org/EFRO/EFRO"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:ns2="http://creativecommons.org/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:owl1="http://purl.org/cerif/frapo/owl:"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:cerro="http://purl.org/cerif/cerro/"
     xmlns:frapo="http://purl.org/cerif/frapo/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:sw-vocab-status="http://www.w3.org/2003/06/sw-vocab-status/">
    <owl:Ontology rdf:about="http://www.w3id.org/EFRO/EFRO">
        <owl:imports rdf:resource="http://purl.org/cerif/frapo/"/>
        <owl:imports rdf:resource="http://purl.org/vocab/aiiso/schema#"/>
        <owl:imports rdf:resource="http://purl.org/vocab/vann/"/>
        <owl:imports rdf:resource="http://www.w3.org/2003/06/sw-vocab-status/ns"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <ns2:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dc:contributor>Ikechukwu Nkisi-Orji</dc:contributor>
        <dc:coverage>Educational domain including student management, program administration, funding allocation, and assessment tracking.</dc:coverage>
        <dc:rights>This ontology is licensed under the Creative Commons Attribution 4.0 International License (CC-BY 4.0). You are free to share and adapt this ontology, provided that you give appropriate credit. More details: https://creativecommons.org/licenses/by/4.0/</dc:rights>
        <terms:contributor>David Corsar</terms:contributor>
        <terms:creator>Umair Arshad</terms:creator>
        <terms:description>An ontology for modeling educational institutions&apos; funding, assessment, and student management systems. It captures the relationships between students, educational programs, funding mechanisms, and assessment processes. The ontology supports tracking student statuses, attendance, progression, and funding eligibility while maintaining compliance with educational standards and reporting requirements.</terms:description>
        <terms:format>rdf/xml</terms:format>
        <terms:language>en</terms:language>
        <terms:title>Educational Funding Regulations Ontology (EFRO)</terms:title>
        <rdfs:comment>This ontology models:
    1. Student enrollment and status tracking
    2. Educational program structures and requirements
    3. Funding mechanisms and eligibility criteria
    4. Assessment and progression tracking
    5. Attendance monitoring systems
    6. Documentation and compliance requirements
    7. Administrative processes and support systems</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://creativecommons.org/ns#license -->


    <owl:AnnotationProperty rdf:about="http://creativecommons.org/ns#license"/>
    


    <!-- http://purl.org/dc/elements/1.1/coverage -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/coverage"/>
    


    <!-- http://purl.org/dc/terms/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#example -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#example"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3id.org/EFRO/EFRO#belongsTo -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#belongsTo">
        <owl:inverseOf rdf:resource="http://www.w3id.org/EFRO/EFRO#includes"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Learning"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <terms:description xml:lang="en">Indicates an entity&apos;s relationship to a parent entity, such as a student belonging to an institution or a funding scheme.</terms:description>
        <rdfs:label>belongsTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#constrains -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#constrains">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#correspondsToLearningAim -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#correspondsToLearningAim">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#ClassRegister"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <rdfs:label>correspondsToLearningAim</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#defines -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#defines">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingImpact"/>
        <rdfs:label>defines</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#enrolledIn -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#enrolledIn">
        <owl:inverseOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasEnrollee"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
        <terms:description xml:lang="en">Represents a student&apos;s enrolment in a specific programme, institution, or funding category​.</terms:description>
        <rdfs:label>enrolledIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#evaluates -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#evaluates">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#forProgramme -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#forProgramme">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
        <rdfs:comment>for programme</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#fulfills -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#fulfills">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Evidence"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#Obligation"/>
        <rdfs:label>fulfills</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#givernedBy -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#givernedBy">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingRegulations"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:label>giverned By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAchievementStatus -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAchievementStatus">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#AchievementStatus"/>
        <terms:description xml:lang="en">Represents the status of a student’s academic or vocational achievement, tracking progress and outcomes.</terms:description>
        <rdfs:label>hasAchievementStatus</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasActiveParticipation -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasActiveParticipation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasParticipationStatus"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#ActiveParticipationStatus"/>
        <terms:description xml:lang="en">Indicates that a student is actively engaged in their learning and meets attendance requirements.</terms:description>
        <rdfs:label>hasActiveParticipation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasActualAttendance -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasActualAttendance">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasAttendanceRecord"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#ActualAttendanceActivity"/>
        <terms:description xml:lang="en">Tracks the actual attendance hours recorded for a student, ensuring compliance with funding regulations​.</terms:description>
        <rdfs:label>hasActualAttendance</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAdminSystemAccount -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAdminSystemAccount">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasOnlineAccount"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Staff"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/OnlineAccount"/>
        <rdfs:label>hasAdminSystemAccount</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAimStatus -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAimStatus">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <rdfs:label>has aim status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasApplicationStatus -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasApplicationStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasStatus"/>
        <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Application"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <rdfs:label>hasApplicationStatus</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasApplicationType -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasApplicationType">
        <rdfs:domain rdf:resource="http://purl.org/cerif/frapo/Application"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <rdfs:label>hasApplicationType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasApprenticeshipFunding -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasApprenticeshipFunding">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasFundingElement"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Learning"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Student"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#ApprenticeshipFunding"/>
        <terms:description xml:lang="en">Denotes funding specifically allocated for apprenticeship programmes, covering training, assessment, and employer incentives.</terms:description>
        <rdfs:label>hasApprenticeshipFunding</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAssessmentRecord -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAssessmentRecord">
        <owl:inverseOf rdf:resource="http://www.w3id.org/EFRO/EFRO#isAssessmentRecord"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#AssessmentActivity"/>
        <terms:description xml:lang="en">Links a student’s assessment records, such as examinations, assignments, and overall performance tracking.</terms:description>
        <rdfs:label>hasAssessmentRecord</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAssignmentCompletion -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAssignmentCompletion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasAssessmentRecord"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#AssignmentActivity"/>
        <terms:description xml:lang="en">Indicates that a student has submitted and completed a required assignment as part of their programme​.</terms:description>
        <rdfs:label>hasAssignmentCompletion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAssociatedDocument -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAssociatedDocument">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Obligation"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#Document"/>
        <terms:description xml:lang="en">Links an entity (e.g., student, programme, funding application) to a supporting document such as a learning agreement, enrolment form, or funding claim.</terms:description>
        <rdfs:label>hasAssociatedDocument</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAttendanceEvidence -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAttendanceEvidence">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#LastAttendanceActivity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#ClassRegister"/>
        <terms:description xml:lang="en">Refers to records that demonstrate a student&apos;s attendance in learning activities, which is essential for funding eligibility.</terms:description>
        <rdfs:label>hasAttendanceEvidence</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasAttendanceRecord -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasAttendanceRecord">
        <owl:inverseOf rdf:resource="http://www.w3id.org/EFRO/EFRO#isAttendanceRecordOf"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#AttendanceActivity"/>
        <terms:description xml:lang="en">Represents a student’s attendance data, used to validate funding claims and student engagement.</terms:description>
        <rdfs:label>hasAttendanceRecord</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasClassRegister -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasClassRegister">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#LastAttendanceActivity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#ClassRegister"/>
        <terms:description xml:lang="en">Links a student or programme to an official attendance register used to record participation​.</terms:description>
        <rdfs:label>hasClassRegister</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasCompletionStatus -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasCompletionStatus">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#CompletionStatus"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Student"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#CompletionStatus"/>
        <terms:description xml:lang="en">Represents the completion status of a student&apos;s learning aim or study programme, whether successful, withdrawn, or incomplete.</terms:description>
        <rdfs:label>hasCompletionStatus</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasComplianceRequirement -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasComplianceRequirement">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasLegalDuty"/>
        <owl:inverseOf rdf:resource="http://www.w3id.org/EFRO/EFRO#isComplianceRequirementFor"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundedInstitution"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#ComplianceObligation"/>
        <terms:description xml:lang="en">Defines the legal and regulatory requirements institutions must meet to receive and maintain funding.</terms:description>
        <rdfs:label>hasComplianceRequirement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasDistanceLearningProvision -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasDistanceLearningProvision">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasLearningMode"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Learning"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#DistanceLearningProvision"/>
        <terms:description xml:lang="en">Represents learning that occurs remotely, without requiring physical attendance at an institution.</terms:description>
        <rdfs:label>hasDistanceLearningProvision</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasDocumentation -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasDocumentation">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Reason"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#Document"/>
        <terms:description xml:lang="en">Indicates the presence of required supporting documents for enrolment, funding applications, or compliance.</terms:description>
        <rdfs:label>hasDocumentation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasDoubleFundingRestriction -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasDoubleFundingRestriction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasFundingCondition"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#DoubleFundingRestriction"/>
        <terms:description xml:lang="en">Prevents a student from being funded simultaneously by multiple programmes or institutions.</terms:description>
        <rdfs:label>hasDoubleFundingRestriction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasESFAFunding -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasESFAFunding">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasFundingElement"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Learning"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Student"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#ESFAFunding"/>
        <terms:description xml:lang="en">Indicates financial support provided by the Education and Skills Funding Agency (ESFA) for 16-19 education, adult learning, and traineeships.</terms:description>
        <rdfs:label>hasESFAFunding</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasEarlyCompletion -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasEarlyCompletion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasAchievementStatus"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#AchievementStatus"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#EarlyCompletionStatus"/>
        <terms:description xml:lang="en">Indicates that a student has completed their programme earlier than planned, which may impact funding adjustments​.</terms:description>
        <rdfs:label>hasEarlyCompletion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasEarlyCompletionStatus -->


    <owl:ObjectProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasEarlyCompletionStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasParticipationStatus"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3id.org/EFRO/EFRO#EarlyCompletionStatus"/>
        <terms:description xml:lang="en">Captures the status of a student who has completed their programme ahead of the planned schedule.</terms:description>
        <rdfs:label>hasEarlyCompletionStatus</rdfs:label>
    </owl:ObjectProperty>
    


