    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasDescription">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Activity"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Document"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#EducationalEntity"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Learning"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Obligation"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Person"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Reason"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Status"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Provides a textual explanation or detailed information about a particular funding-related concept, activity, or rule within the educational funding framework.</terms:description>
        <rdfs:label>hasDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasDurationValue -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasDurationValue">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <terms:description xml:lang="en">Represents the time-related aspects of a study programme, learning aim, or funding period.</terms:description>
        <rdfs:label>hasDurationValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasEarliestStartDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasEarliestStartDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Represents the first possible start date for a study programme, determining the qualifying period for funding eligibility.</terms:description>
        <rdfs:label>hasEarliestStartDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasEndDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasEndDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingYear"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">The official or anticipated end date of a student&apos;s learning aim, used for funding and tracking progression​.</terms:description>
        <rdfs:label>hasEndDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasEnrollmentStatus -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasEnrollmentStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasStatusValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentStatus"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Active</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Completed</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Withdrawn</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Denotes whether a student is currently enrolled, deferred, or has withdrawn from the programme.</terms:description>
        <rdfs:label>hasEnrollmentStatus</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasExcludedHours -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasExcludedHours">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasHoursValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#ExcludedHours"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <terms:description xml:lang="en">Represents learning hours that are excluded from funding claims due to non-qualification activities, self-study, or ineligible activities.</terms:description>
        <rdfs:label>hasExcludedHours</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasFundingID -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasFundingID">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasIdentifier"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Identifies the unique funding reference number assigned to a specific student or programme for tracking financial allocations.</terms:description>
        <rdfs:label>hasFundingID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasFundingStatus -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasFundingStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasStatusValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingStatus"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Funded</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>NotFunded</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>PartiallyFunded</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Reflects whether a student’s funding is active, suspended, reduced, or withdrawn based on their participation and eligibility.</terms:description>
        <rdfs:label>hasFundingStatus</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasFundingValue -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasFundingValue">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <terms:description xml:lang="en">Represents monetary allocations related to student enrolment, learning aims, and retention for funding purposes.</terms:description>
        <rdfs:label>hasFundingValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasHoursPlanned -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasHoursPlanned">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasHoursValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <terms:description xml:lang="en">Defines the expected learning hours allocated to a study programme at the start of the academic year​.</terms:description>
        <rdfs:label>hasHoursPlanned</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasHoursValue -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasHoursValue">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <terms:description xml:lang="en">Represents the number of learning hours recorded for funding purposes, including planned, adjusted, or excluded hours.</terms:description>
        <rdfs:label>hasHoursValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasIdentifier -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasIdentifier">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Activity"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Document"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#EducationalEntity"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Learning"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Obligation"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Person"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Reason"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Status"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Represents unique identifiers assigned to various entities within the educational funding framework.</terms:description>
        <rdfs:label>hasIdentifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasInstitutionID -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasInstitutionID">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasIdentifier"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#EducationalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Specifies the unique identifier for an educational institution, used in funding and regulatory reporting​.</terms:description>
        <rdfs:label>hasInstitutionID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasLastAttendanceDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasLastAttendanceDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#LastAttendanceActivity"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Student"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Captures the last recorded date a student attended a learning activity before withdrawal, completion, or absence​.</terms:description>
        <rdfs:label>hasLastAttendanceDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasMetFundingCriteria -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasMetFundingCriteria">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <terms:description xml:lang="en">Indicates whether a student or programme has met the eligibility criteria for funding, based on the ESFA guidelines​.</terms:description>
        <rdfs:label xml:lang="en">hasMetFundingCriteria</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasNonAttendanceReason -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasNonAttendanceReason">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasDescription"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#NonAttendanceReason"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Describes the reason for a student’s absence from their study programme, which may impact funding eligibility, retention, or planned hours.</terms:description>
        <rdfs:label>hasNonAttendanceReason</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasOriginalPlannedHours -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasOriginalPlannedHours">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <terms:description xml:lang="en">Captures the initial number of planned learning hours assigned to a student before any adjustments​.</terms:description>
        <rdfs:label>hasOriginalPlannedHours</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasPlannedDuration -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasPlannedDuration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasDurationValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Learning"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Specifies the expected length of a programme or course as originally planned and recorded in funding records​.</terms:description>
        <rdfs:label>hasPlannedDuration</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasPlannedEndDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasPlannedEndDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Indicates the expected completion date set at the start of the study programme, which may differ from the actual end date</terms:description>
        <rdfs:label>hasPlannedEndDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasProgrammeID -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasProgrammeID">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasIdentifier"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Represents the identifier assigned to a specific study programme or qualification pathway​.</terms:description>
        <rdfs:label>hasProgrammeID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasProgressionDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasProgressionDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Records the date a student progresses to the next stage of their learning pathway, such as transitioning from one qualification to another.</terms:description>
        <rdfs:label>hasProgressionDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasQualifyingDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasQualifyingDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#QualifyingPeriod"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Defines the date when a student becomes eligible for funding by meeting attendance and participation requirements​.</terms:description>
        <rdfs:label>hasQualifyingDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasQualifyingPeriodDuration -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasQualifyingPeriodDuration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasDurationValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#QualifyingPeriod"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Represents the minimum period a student must remain enrolled to qualify for funding, ensuring institutions receive appropriate allocations.</terms:description>
        <rdfs:label>hasQualifyingPeriodDuration</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasReductionAmount -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasReductionAmount">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasFundingValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.00</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Captures the amount deducted from an allocated funding value due to early withdrawals, non-completion, or ineligible hours​.</terms:description>
        <rdfs:label>hasReductionAmount</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasReference -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasReference">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Obligation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Provides a unique identifier or reference number for records, learning aims, or funding-related documentation.</terms:description>
        <rdfs:label>hasReference</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasRetentionValue -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasRetentionValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasFundingValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.00</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.00</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Indicates the funding value assigned for students who meet retention criteria and complete the required duration of study.</terms:description>
        <rdfs:label>hasRetentionValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasStaffID -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasStaffID">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasIdentifier"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Staff"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Denotes a unique identifier for staff members involved in delivering funded study programmes​.</terms:description>
        <rdfs:label>hasStaffID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasStartDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasStartDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Activity"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Learning"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">The official commencement date of a student&apos;s learning aim or study programme​.</terms:description>
        <rdfs:label>hasStartDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasStatusValue -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasStatusValue">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Represents the current status of a student&apos;s enrolment, funding, or participation in a learning programme.</terms:description>
        <rdfs:label>hasStatusValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasStudentID -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasStudentID">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasIdentifier"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Provides a unique reference number for students enrolled in funded programmes, ensuring accurate tracking of learning progress and funding eligibility.</terms:description>
        <rdfs:label>hasStudentID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasTemporalValue -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasTemporalValue">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Represents date-related aspects of a student’s learning journey, including start, end, and milestone dates.</terms:description>
        <rdfs:label>hasTemporalValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasTotalPlannedHours -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasTotalPlannedHours">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasHoursValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">40</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Denotes the cumulative planned hours for a student’s study programme, including all qualification and eligible non-qualification activities​.</terms:description>
        <rdfs:label>hasTotalPlannedHours</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasWeeksDuration -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasWeeksDuration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasDurationValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <terms:description xml:lang="en">Indicates the duration of a learning aim or study programme in terms of weeks, often used in eligibility and funding calculations.</terms:description>
        <rdfs:label>hasWeeksDuration</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasWithdrawReason -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasWithdrawReason">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasDescription"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalReason"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <terms:description xml:lang="en">Indicates why a student has withdrawn from a study programme, such as financial difficulties, personal reasons, or transfer to another institution.</terms:description>
        <rdfs:label>hasWithdrawReason</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasWithdrawalDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasWithdrawalDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/EFRO/EFRO#hasTemporalValue"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Specifies the date when a student officially withdraws from a study programme, impacting funding calculations​.</terms:description>
        <rdfs:label>hasWithdrawalDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasWithdrawalFromProgrammeDate -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasWithdrawalFromProgrammeDate">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <terms:description xml:lang="en">Specifies the date when a student officially withdraws from a study programme​.</terms:description>
        <rdfs:label>hasWithdrawalFromProgrammeDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#hasWithdrawnFromEntireProgramme -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#hasWithdrawnFromEntireProgramme">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <terms:description xml:lang="en">A boolean property that denotes whether a student has withdrawn from all learning aims within their study programme​.</terms:description>
        <rdfs:label>hasWithdrawnFromEntireProgramme</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#isCountedAsGuidedLearning -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#isCountedAsGuidedLearning">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <terms:description xml:lang="en">Identifies whether a learning activity qualifies as guided learning hours (GLH) for funding purposes​.</terms:description>
        <rdfs:label>isCountedAsGuidedLearning</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#isCountedAsPlannedHours -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#isCountedAsPlannedHours">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <terms:description xml:lang="en">Determines whether a specific learning activity is included in the total planned hours recorded for funding​.</terms:description>
        <rdfs:label>isCountedAsPlannedHours</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#isValidLearningActivity -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#isValidLearningActivity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#ExaminationActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <terms:description xml:lang="en">Specifies whether an activity meets the funding body’s criteria for a valid learning activity, ensuring eligibility for funding​.</terms:description>
        <rdfs:label>isValidLearningActivity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/EFRO/EFRO#progressedInWeek -->


    <owl:DatatypeProperty rdf:about="http://www.w3id.org/EFRO/EFRO#progressedInWeek">
        <rdfs:domain rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


