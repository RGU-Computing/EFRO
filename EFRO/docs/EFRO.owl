        <rdfs:label>ApprenticeshipProvider</rdfs:label>
        <skos:example>Apprenticeship providers must ensure that work placements and educational content meet compliance standards for funding​</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ApprenticeshipQualification -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ApprenticeshipQualification">
