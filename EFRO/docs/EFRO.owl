        <rdfs:label>ASLevel</rdfs:label>
        <skos:example>AS Level qualifications in Mathematics or English.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AcademicProgressionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AcademicProgressionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionActivity"/>
        <terms:description>Progression activities focusing on advancing a student’s academic qualifications or study program.</terms:description>
        <rdfs:label>AcademicProgressionActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AcademicYear -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AcademicYear">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePeriod"/>
        <terms:description>A standard 12-month period during which educational activities occur, typically running from September to August.</terms:description>
        <rdfs:label>AcademicYear</rdfs:label>
        <skos:example>The academic year structure used to plan funding, timetables, and reporting for study programs​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AchievementStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AchievementStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <terms:description>The status reflecting whether a student has achieved the intended outcomes of their study program or qualification.</terms:description>
        <rdfs:label>AchievementStatus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ActiveParticipationStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ActiveParticipationStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ParticipationStatus"/>
        <terms:description>Indicates that the student is actively engaging in their study program.</terms:description>
        <rdfs:label>ActiveParticipationStatus</rdfs:label>
        <skos:example>Active participation demonstrated through attendance and assignment submissions​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ActiveStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ActiveStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentStatus"/>
        <owl:disjointWith rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawnStatus"/>
        <terms:description>Indicates that the student is actively enrolled and participating in their study program.</terms:description>
        <rdfs:label>ActiveStatus</rdfs:label>
        <skos:example>Students attending planned hours and meeting progression milestones.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Activity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Activity">
        <terms:description xml:lang="en">Represents any task, process, or event associated with administering, managing, or supporting educational programs and funding compliance. It serves as the parent class for specialized activities related to attendance, progression, recording, withdrawal, and enrollment.</terms:description>
        <rdfs:label>Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ActualAttendanceActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ActualAttendanceActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AttendanceActivity"/>
        <terms:description>The actual participation or presence of a student in scheduled study programs or activities.</terms:description>
        <rdfs:label>ActualAttendanceActivity</rdfs:label>
        <skos:example>Weekly attendance logs submitted for ESFA compliance.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ActualAttendancePeriod -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ActualAttendancePeriod">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAspect"/>
        <terms:description>The period during which a student has actively participated in their study program.</terms:description>
        <rdfs:label>ActualAttendancePeriod</rdfs:label>
        <skos:example>Attendance records used for funding validation​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AdjustedPlannedHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AdjustedPlannedHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <terms:description>Revised planned hours to reflect changes in the student’s program, such as early completion or withdrawal.</terms:description>
        <rdfs:label>AdjustedPlannedHours</rdfs:label>
        <skos:example>Reduced hours due to a student completing a program early​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AdministrativeActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AdministrativeActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <terms:description xml:lang="en">Covers activities related to the management and administration of funding compliance, institutional governance, and student records.en</terms:description>
        <rdfs:label>AdministrativeActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AdministrativeMeeting -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AdministrativeMeeting">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#GeneralAssistanceActivity"/>
        <owl:disjointWith rdf:resource="http://www.w3id.org/EFRO/EFRO#GuidedLearning"/>
        <rdfs:comment>Meeting for general administrative or attendance discussion that cannot be counted as planned hours or used as withdrawal date.</rdfs:comment>
        <rdfs:label>AdministrativeMeeting</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AdministrativeStaff -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AdministrativeStaff">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Staff"/>
        <terms:description>Staff responsible for managing records, compliance, and administrative tasks to meet ESFA funding and reporting requirements.</terms:description>
        <rdfs:label>AdministrativeStaff</rdfs:label>
        <skos:example>Staff members responsible for submitting ILR data and maintaining attendance records.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AfterInitialSixWeeks -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AfterInitialSixWeeks">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeCondition"/>
        <terms:description>A time condition indicating the period after the initial six weeks of a program. This is often used to determine funding eligibility, as students retained beyond this period qualify for funding.</terms:description>
        <rdfs:label>AfterInitialSixWeeks</rdfs:label>
        <skos:example>Funding claims are validated for students continuing their study program after the initial six-week period.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Apprentice -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Apprentice">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Participant"/>
        <terms:description>A participant enrolled in an apprenticeship program that combines work-based training with classroom instruction, funded through apprenticeship standards.</terms:description>
        <rdfs:label>Apprentice</rdfs:label>
        <skos:example>A digital marketing apprentice completing an ESFA-funded apprenticeship​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ApprenticeshipFunding -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ApprenticeshipFunding">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingType"/>
