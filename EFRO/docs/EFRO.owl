        <terms:description>Qualifications awarded for completing apprenticeship standards or frameworks.</terms:description>
        <rdfs:label>ApprenticeshipQualification</rdfs:label>
        <skos:example>Level 3 Engineering Technician Standard​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AssessmentActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AssessmentActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <terms:description xml:lang="en">Covers activities related to evaluating student progress, performance, and completion of educational aims.</terms:description>
        <rdfs:label>AssessmentActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AssignmentActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AssignmentActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AssessmentActivity"/>
        <terms:description xml:lang="en">Involves the planning, submission, and grading of assignments as part of a student’s learning or qualification aim.</terms:description>
        <rdfs:label>AssignmentActivity</rdfs:label>
        <skos:example xml:lang="en">A marked essay that contributes to the student’s qualification​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AttendanceActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AttendanceActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <terms:description>Covers activities related to tracking, recording, and managing student attendance for compliance and funding purposes.</terms:description>
        <rdfs:label>AttendanceActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AttendanceReport -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AttendanceReport">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ReportDocument"/>
        <terms:description>A document summarizing student attendance over a defined period. It serves as evidence of engagement and retention for funding purposes.</terms:description>
        <rdfs:label>AttendanceReport</rdfs:label>
        <skos:example>Reports generated from class registers or attendance monitoring systems for audit purposes.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AttendanceStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AttendanceStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <terms:description>Reflects the attendance record of a student, including periods of active participation and absences.</terms:description>
        <rdfs:label>AttendanceStatus</rdfs:label>
        <skos:example>Students with attendance below the threshold may not qualify for full funding.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#AuditDocument -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#AuditDocument">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ComplianceDocument"/>
        <terms:description>A document produced for or during an audit to validate compliance with ESFA funding regulations. It includes records that justify funding claims, attendance, planned hours, and other institutional obligations as outlined in the guidance.</terms:description>
        <rdfs:label>AuditDocument</rdfs:label>
        <skos:example>Documents demonstrating compliance with planned hours or evidence of student eligibility during audits.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Authority -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Authority">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EducationalEntity"/>
        <terms:description>A governing or administrative entity responsible for overseeing and ensuring compliance with educational policies, funding regulations, and operational standards.</terms:description>
        <rdfs:label>Authority</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#BeforeQualifyingStartPeriod -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#BeforeQualifyingStartPeriod">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeCondition"/>
        <terms:description>A time condition representing activities or periods that occur before the qualifying start period. This period does not count towards funding eligibility or compliance.</terms:description>
        <rdfs:label>BeforeQualifyingStartPeriod</rdfs:label>
        <skos:example>Activities undertaken by a student before their formal enrollment or qualifying attendance period.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ClassRegister -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ClassRegister">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#RecordDocument"/>
        <terms:description>A document recording student attendance for specific sessions, including details like the date, time, duration, and tutor information. This is key evidence for compliance with funding regulations.</terms:description>
        <rdfs:label>ClassRegister</rdfs:label>
        <skos:example>Registers showing attendance during in-person or virtual sessions, signed by tutors or verified electronically.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#CollegeStaff -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#CollegeStaff">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Staff"/>
        <terms:description>Employees working in a college setting, including administrative, teaching, and support staff, who ensure compliance with ESFA regulations.</terms:description>
        <rdfs:label>CollegeStaff</rdfs:label>
        <skos:example>compliance officer ensuring adherence to planned hours and funding requirements in a college​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#CompletedEarlyStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#CompletedEarlyStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#CompletionStatus"/>
        <terms:description>Status showing the student completed their study program ahead of the planned schedule.</terms:description>
        <rdfs:label>CompletedEarlyStatus</rdfs:label>
        <skos:example>Early completion adjustments are applied to funding in such cases​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#CompletedSuccessfullyStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#CompletedSuccessfullyStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#CompletionStatus"/>
        <terms:description>Indicates the successful completion of the program, including achieving all learning aims.</terms:description>
        <rdfs:label>CompletedSuccessfullyStatus</rdfs:label>
        <skos:example>Students completing their qualifications with satisfactory attendance and assessment completion.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#CompletionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#CompletionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <terms:description>Activities related to the successful or partial completion of a study program or educational objective.</terms:description>
        <rdfs:label>CompletionActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#CompletionStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#CompletionStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <terms:description>Indicates whether a student has completed their study program as planned, early, or not at all.</terms:description>
        <rdfs:label>CompletionStatus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ComplianceCriterion -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ComplianceCriterion">
        <rdfs:label>Compliance Criterion</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ComplianceDocument -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ComplianceDocument">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Document"/>
        <terms:description>A category of documents specifically focused on meeting regulatory, legal, and funding requirements. These documents ensure adherence to the ESFA guidelines and audit standards.</terms:description>
        <rdfs:label>ComplianceDocument</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ComplianceObligation -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ComplianceObligation">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Obligation"/>
        <terms:description>A requirement to adhere to funding and regulatory guidelines to ensure that funding is used appropriately and that study programs meet the required standards.</terms:description>
        <rdfs:label>ComplianceObligation</rdfs:label>
        <skos:example>Ensuring planned hours are accurately recorded, students meet qualifying criteria, and institutions adhere to audit requirements​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ContinueFunding -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ContinueFunding">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingImpact"/>
        <rdfs:label>Continue Funding</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ContinuingStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ContinuingStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionStatus"/>
        <terms:description>Indicates that the student is progressing as planned within their current study program.</terms:description>
        <rdfs:label>ContinuingStatus</rdfs:label>
        <skos:example>A student moving from Year 1 to Year 2 of their T Level program.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#DateOfWithdrawal -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#DateOfWithdrawal">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePoint"/>
        <terms:description>The specific date on which a student withdraws from their study program. This date is critical for recalculating funding claims.</terms:description>
        <rdfs:label>DateOfWithdrawal</rdfs:label>
        <skos:example>Withdrawal forms must specify the official date of withdrawal for funding adjustments.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#DistanceLearningProvision -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#DistanceLearningProvision">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningMode"/>
        <owl:disjointWith rdf:resource="http://www.w3id.org/EFRO/EFRO#TraditionalProvision"/>
        <terms:description>Learning activities delivered remotely, typically through online platforms, without the need for physical attendance.</terms:description>
        <rdfs:label>DistanceLearningProvision</rdfs:label>
        <skos:example>Online modules for students unable to attend in person​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Document -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Document">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Evidence"/>
        <terms:description>Represents any formal written, electronic, or recorded material used to support compliance, funding, audit, and administrative processes. Documents in this hierarchy serve as evidence, records, or reports within the educational context.</terms:description>
        <rdfs:label>Document</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#DocumentationActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#DocumentationActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AdministrativeActivity"/>
        <terms:description xml:lang="en">The process of creating, collecting, and maintaining records and documents to support compliance with funding, attendance, and progression requirements.</terms:description>
        <rdfs:label>DocumentationActivity</rdfs:label>
        <skos:example xml:lang="en">Maintaining funding evidence documents for audits.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#DoubleFundingRestriction -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#DoubleFundingRestriction">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingCondition"/>
        <terms:description>A restriction preventing institutions from claiming funding for the same student or activity from multiple sources.</terms:description>
        <rdfs:label>DoubleFundingRestriction</rdfs:label>
        <skos:example>Restrictions ensuring that students enrolled in apprenticeship programs are not double-funded through other ESFA-funded programs​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ESFAFunding -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ESFAFunding">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingType"/>
        <terms:description>General funding provided by the ESFA for study programs, qualifications, and institutional support.</terms:description>
        <rdfs:label>ESFAFunding</rdfs:label>
        <skos:example>16-19 study program funding, including high-needs student allocations​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EarlyCompletionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EarlyCompletionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#CompletionActivity"/>
        <terms:description>An activity involving the completion of a program or objective earlier than the planned schedule.</terms:description>
        <rdfs:label>EarlyCompletionActivity</rdfs:label>
        <skos:example>Adjusting funding claims for students who complete qualifications ahead of schedule​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EarlyCompletionAdjustment -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EarlyCompletionAdjustment">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingAdjustment"/>
        <terms:description>An adjustment applied when a student completes their program earlier than planned. Funding is recalculated based on the actual duration of attendance and the completion criteria.</terms:description>
        <rdfs:label>EarlyCompletionAdjustment</rdfs:label>
        <skos:example>A reduced funding claim if a student achieves their qualification ahead of schedule without completing the planned hours​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EarlyCompletionStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EarlyCompletionStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AchievementStatus"/>
        <terms:description>Indicates that a student has completed their qualification or study program earlier than the planned duration.</terms:description>
        <rdfs:label>EarlyCompletionStatus</rdfs:label>
        <skos:example>A student completing their apprenticeship program ahead of schedule​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EducationProvider -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EducationProvider">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EducationalEntity"/>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <terms:description>An organization that delivers educational programs, apprenticeships, or training services. Providers are responsible for meeting funding conditions and delivering high-quality education.</terms:description>
        <rdfs:label>Education Provider</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EducationalEntity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EducationalEntity">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <terms:description>Represents any organization, body, or institution involved in the delivery, regulation, or oversight of education, training, and funding processes.</terms:description>
        <rdfs:label>EducationalEntity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EducationalInstitution -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EducationalInstitution">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Institution"/>
        <rdfs:label>EducationalInstitution</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EducationalProvider -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EducationalProvider">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EducationProvider"/>
        <terms:description>A provider delivering general education programs, including study programs for students aged 16 to 19 or those with Education, Health, and Care (EHC) plans.</terms:description>
        <rdfs:label>EducationalProvider</rdfs:label>
        <skos:example>Colleges offering T Level programs or academic qualifications such as A Levels​</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EmploymentProgressionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EmploymentProgressionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionActivity"/>
        <terms:description>Activities that facilitate transitioning into employment, including work-based training.</terms:description>
        <rdfs:label>EmploymentProgressionActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EnrollmentActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EnrollmentActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <terms:description>Covers all activities associated with registering or enrolling students into educational programs.</terms:description>
        <rdfs:label>EnrollmentActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#EnrollmentStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#EnrollmentStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <terms:description>Reflects the enrollment state of a student within a study program.</terms:description>
        <rdfs:label>EnrollmentStatus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Evidence -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Evidence">
        <rdfs:label>Evidence</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ExaminationActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ExaminationActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AssessmentActivity"/>
        <terms:description xml:lang="en">Activities related to administering, grading, and recording examinations.en</terms:description>
        <rdfs:comment>Assessment of student achievement that counts as learning activity</rdfs:comment>
        <rdfs:label>ExaminationActivity</rdfs:label>
        <skos:example xml:lang="en">Formal assessments conducted for qualifications such as GCSEs or vocational qualifications.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ExaminationDate -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ExaminationDate">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePoint"/>
        <terms:description>The date on which a student completes a formal examination as part of their study program.</terms:description>
        <rdfs:label>ExaminationDate</rdfs:label>
        <skos:example>Institutions report examination dates for students completing final assessments​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ExcludedHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ExcludedHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <terms:description>ours excluded from funding calculations, such as breaks or activities not eligible for funding.</terms:description>
        <rdfs:label>ExcludedHours</rdfs:label>
        <skos:example>Meal breaks or voluntary extracurricular activities​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ExitedStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ExitedStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionStatus"/>
        <terms:description>Indicates that the student has exited the program before completion, potentially for personal reasons, employment, or transfer.</terms:description>
        <rdfs:label>ExitedStatus</rdfs:label>
        <skos:example>A student leaving to join an apprenticeship before completing their current program​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FinanciallyRelatedInstitution -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FinanciallyRelatedInstitution">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Institution"/>
        <terms:description>An institution with financial relationships tied to funding agreements, including direct funding from the ESFA or through subcontracting arrangements.</terms:description>
        <rdfs:label>FinanciallyRelatedInstitution</rdfs:label>
        <skos:example>Colleges or academies receiving ESFA funding for study programs</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FirstDateOfAttendance -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FirstDateOfAttendance">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePoint"/>
        <terms:description>The date when a student first attends their study program, marking the start of their qualifying attendance period.</terms:description>
        <rdfs:label>FirstDateOfAttendance</rdfs:label>
        <skos:example>The first date of attendance must align with the planned program start for funding validation​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FullTimeEducationProgression -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FullTimeEducationProgression">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionType"/>
        <terms:description xml:lang="en">Represents a student&apos;s transition from one level of full-time education to another, such as moving from secondary education to further education or higher education.</terms:description>
        <rdfs:label>FullTimeEducationProgression</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FullTimeProgram -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FullTimeProgram">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/EFRO/EFRO#hasHoursPlanned"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">540</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
        <terms:description>A study program requiring 540 or more planned hours over an academic year, meeting the criteria for full-time funding.</terms:description>
        <rdfs:label>FullTimeProgram</rdfs:label>
        <skos:example>A full-time A Level program for 16-19 students​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FullTimeStudent -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FullTimeStudent">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <owl:disjointWith rdf:resource="http://www.w3id.org/EFRO/EFRO#PartTimeStudent"/>
        <terms:description>A student enrolled in a study program with 540 or more planned hours over an academic year, qualifying as full-time under ESFA funding rules.</terms:description>
        <rdfs:label>FullTimeStudent</rdfs:label>
        <skos:example>A student enrolled in a full-time T Level program​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundedInstitution -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundedInstitution">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Institution"/>
        <terms:description>An institution directly receiving funding from the ESFA to deliver educational programs that comply with the regulations outlined in the funding guidance.</terms:description>
        <rdfs:label>FundedInstitution</rdfs:label>
        <skos:example>FE colleges and sixth forms funded to provide study programs for students aged 16 to 19</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundedStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundedStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingStatus"/>
        <owl:disjointWith rdf:resource="http://www.w3id.org/EFRO/EFRO#NonFundedStatus"/>
        <terms:description>Indicates that the student or program is fully eligible for ESFA funding.</terms:description>
        <rdfs:label>FundedStatus</rdfs:label>
        <skos:example>A 16-19-year-old student meeting all funding criteria for a full-time program.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundedStudent -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundedStudent">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Student"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/EFRO/EFRO#relatedToFundingStatus"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3id.org/EFRO/EFRO#FundedStatus"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <terms:description>A student who meets the criteria for ESFA funding and is included in funding claims submitted by an institution.</terms:description>
        <rdfs:label>FundedStudent</rdfs:label>
        <skos:example>A 17-year-old enrolled in a vocational qualification funded through the 16-19 study program budget​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingAdjustment -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingAdjustment">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <terms:description>Adjustments applied to the allocated funding based on compliance, attendance, or completion criteria. These adjustments may result in increases or reductions in funding based on specific conditions outlined in the ESFA guidance.</terms:description>
        <rdfs:label>FundingAdjustment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingBand -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingBand">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingComponent"/>
        <terms:description>A predefined range of funding amounts based on the type and level of the qualification being delivered. Funding bands ensure consistency in funding allocations for similar programs.</terms:description>
        <rdfs:label>FundingBand</rdfs:label>
        <skos:example>Funding bands for apprenticeships, where the maximum band reflects the funding cap for that qualification.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingClaimReport -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingClaimReport">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ReportDocument"/>
        <terms:description>A report summarizing the funding claims made by an institution, including planned hours, attendance, and eligible activities. This report is critical for validating funding received from the ESFA.</terms:description>
        <rdfs:label>FundingClaimReport</rdfs:label>
        <skos:example>Reports submitted during funding returns to justify the institution’s funding allocation.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingComponent -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingComponent">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <terms:description>Specific elements or criteria that determine the amount of funding allocated to a program or institution. These components are used to calculate and validate funding claims.</terms:description>
        <rdfs:label>FundingComponent</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingCondition -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingCondition">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <terms:description>Specific conditions or rules that must be met to qualify for and retain funding. These conditions ensure institutions comply with ESFA funding regulations.</terms:description>
        <rdfs:label>FundingCondition</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingElement -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingElement">
        <terms:description>Represents any concept, component, or adjustment directly associated with the allocation, distribution, or compliance of educational funding.</terms:description>
        <rdfs:label>FundingElement</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingEvidenceDocument -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingEvidenceDocument">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ComplianceDocument"/>
        <terms:description>A document used to support funding claims by providing evidence of student eligibility, attendance, planned hours, and study program delivery.</terms:description>
        <rdfs:label>FundingEvidenceDocument</rdfs:label>
        <skos:example>Learning agreements, enrollment forms, and timetables used as funding evidence during compliance audits.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingImpact -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingImpact">
        <rdfs:label>Funding Impact</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingObligation -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingObligation">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Obligation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/EFRO/EFRO#hasHoursPlanned"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description>A duty to meet specific conditions to access or retain funding. Institutions must ensure compliance with funding rules, such as delivering eligible qualifications and avoiding double funding.</terms:description>
        <rdfs:label>FundingObligation</rdfs:label>
        <skos:example>Obligations to demonstrate student participation, maintain evidence for planned hours, and ensure timely withdrawals for students who leave programs.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingOverlap -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingOverlap">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingCondition"/>
        <terms:description>A condition addressing potential overlaps in funding claims, ensuring that institutions do not claim funding for overlapping study periods or activities.</terms:description>
        <rdfs:label>FundingOverlap</rdfs:label>
        <skos:example>Overlaps in planned hours between two study programs for the same studen.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingQualifyingPeriod -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingQualifyingPeriod">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingCondition"/>
        <terms:description>A minimum period that students must participate in a study program to qualify for funding.</terms:description>
        <rdfs:label>FundingQualifyingPeriod</rdfs:label>
        <skos:example>Students must attend for a minimum number of days (e.g., 6 weeks) to qualify for ESFA funding.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingRate -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingRate">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingComponent"/>
        <terms:description>The rate of funding allocated per qualification, student, or activity. Funding rates are determined based on qualification types, planned hours, and delivery models.</terms:description>
        <rdfs:label>FundingRate</rdfs:label>
        <skos:example>The rate per hour for study programs or specific funding rates for high-needs students​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingReduction -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingReduction">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingAdjustment"/>
        <terms:description>An adjustment reducing the allocated funding due to non-compliance, insufficient attendance, or failure to meet required criteria.</terms:description>
        <rdfs:label>FundingReduction</rdfs:label>
        <skos:example>A reduction applied if students do not meet the minimum threshold for planned hours or fail to qualify for funding.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingRegulations -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingRegulations">
        <rdfs:label>Funding Regulations</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingStartCriteria -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingStartCriteria">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingCondition"/>
        <terms:description>Criteria that must be met at the start of a program to establish eligibility for funding.</terms:description>
        <rdfs:label>FundingStartCriteria</rdfs:label>
        <skos:example>Students must meet age, residency, and program type criteria at the beginning of their study​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingStartStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingStartStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingStatus"/>
        <terms:description xml:lang="en">Defines the funding eligibility of a student at the beginning of their study programme.
The funding start status is determined by the student&apos;s enrollment date and whether they meet the qualifying period of attendance required to receive funding​.
This status also ensures that students are only funded at one institution at a time to prevent duplicate funding claims.</terms:description>
        <rdfs:label>FundingStartStatus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <terms:description>Represents the funding eligibility of a student or program based on compliance with ESFA rules.</terms:description>
        <rdfs:label>FundingStatus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingType -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingType">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingElement"/>
        <terms:description>The type of funding allocated based on the program, qualification, or support being delivered. Funding types ensure alignment with ESFA rules for various educational pathways.</terms:description>
        <rdfs:label>FundingType</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#FundingYear -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#FundingYear">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePeriod"/>
        <terms:description>The 12-month period during which funding allocations and claims are calculated, typically aligned with the fiscal year.</terms:description>
        <rdfs:label>FundingYear</rdfs:label>
        <skos:example>ESFA funding allocations for the 2024/25 funding year​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#GeneralAdministrativeActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#GeneralAdministrativeActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AdministrativeActivity"/>
        <terms:description xml:lang="en">Activities that provide overall administrative support to the institution, ensuring smooth operations and adherence to ESFA guidelines.</terms:description>
        <rdfs:label>GeneralAdministrativeActivity</rdfs:label>
        <skos:example xml:lang="en">Scheduling student sessions, maintaining compliance records, and submitting data returns​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#GeneralAssistanceActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#GeneralAssistanceActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <rdfs:comment>Activities that provide general assistance or support but are not part of guided learning.</rdfs:comment>
        <rdfs:label>GeneralAssistanceActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#GuidedLearning -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#GuidedLearning">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningMode"/>
        <terms:description>Planned learning hours under the direct supervision of a tutor or instructor.</terms:description>
        <rdfs:label>GuidedLearning</rdfs:label>
        <skos:example>Classroom teaching sessions and practical workshops​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#InitialASLevel -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#InitialASLevel">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Qualification"/>
        <terms:description>An AS Level qualification pursued at the start of a study program.</terms:description>
        <rdfs:label>InitialASLevel</rdfs:label>
        <skos:example>AS Level Chemistry taken as an initial qualification during a two-year program.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#InitialPlannedHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#InitialPlannedHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <terms:description>The originally planned hours recorded at the start of a study program.</terms:description>
        <rdfs:label>InitialPlannedHours</rdfs:label>
        <skos:example>Timetabled hours based on the initial agreement between the student and institution​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Institution -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Institution">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EducationalEntity"/>
        <terms:description>An educational organization that delivers funded programs or training to students, including schools, colleges, and academies.</terms:description>
        <rdfs:label>Institution</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LastAttendanceActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LastAttendanceActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AttendanceActivity"/>
        <terms:description>The last recorded attendance activity for a student, marking their most recent engagement.</terms:description>
        <rdfs:label>LastAttendanceActivity</rdfs:label>
        <skos:example>Used for calculating funding adjustments after a withdrawal.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LastAttendanceDate -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LastAttendanceDate">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePoint"/>
        <terms:description>The most recent date on which a student attended their study program. This date is used to determine withdrawal or early completion status.</terms:description>
        <rdfs:label>LastAttendanceDate</rdfs:label>
        <skos:example>A student’s funding eligibility ends based on their last recorded attendance date if they withdraw.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LastExam -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LastExam">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AssessmentActivity"/>
        <terms:description>Represents the final or most recent exam undertaken by a student in a study program.</terms:description>
        <rdfs:label>LastExam</rdfs:label>
        <skos:example>The last exam completed before a withdrawal or completion​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LastMarkedAssignment -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LastMarkedAssignment">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AssessmentActivity"/>
        <terms:description>An assignment activity that represents the most recently evaluated and graded assignment for a student.</terms:description>
        <rdfs:label>LastMarkedAssignment</rdfs:label>
        <skos:example>A student’s final marked project before program completion.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Learning -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Learning">
        <terms:description>Represents all aspects of educational and training activities, including the aims, aspects, modes, qualifications, and study programs. It serves as a broad category encompassing the fundamental components of the educational process.</terms:description>
        <rdfs:label>Learning</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LearningAim -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LearningAim">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>
        <terms:description>Refers to the specific educational or training objectives for a student, which may include qualifications or skills development. These aims guide the structure of study programs and funding eligibility.</terms:description>
        <rdfs:label>LearningAim</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LearningAspect -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LearningAspect">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Learning"/>
        <terms:description>Represents the measurable components of a learning aim, such as attendance, planned hours, and timetabled activities, which are essential for funding and compliance.</terms:description>
        <rdfs:label>LearningAspect</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LearningMode -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LearningMode">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#DistanceLearningProvision"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#GuidedLearning"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#OpenLearningProvision"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#TraditionalProvision"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Learning"/>
        <terms:description>The mode of delivery for learning activities, indicating whether the program is delivered in person, online, or in other formats.</terms:description>
        <rdfs:label>LearningMode</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#LocalAuthority -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#LocalAuthority">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Authority"/>
        <terms:description>A regional administrative body responsible for funding and regulating educational institutions, typically for maintained schools and programs within their jurisdiction.</terms:description>
        <rdfs:label>LocalAuthority</rdfs:label>
        <skos:example>Local authorities provide funding to schools and oversee compliance with ESFA regulations for young people’s education</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#NewStart -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#NewStart">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentActivity"/>
        <terms:description>The enrollment of a student starting a new program or qualification aim.</terms:description>
        <rdfs:label>NewStart</rdfs:label>
        <skos:example>A new enrollment after transferring institutions​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#NonAttendanceActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#NonAttendanceActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AttendanceActivity"/>
        <terms:description>Records events where students fail to attend scheduled activities and addresses their reasons for absence.</terms:description>
        <rdfs:label>NonAttendanceActivity</rdfs:label>
        <skos:example>Logs documenting student absences due to personal reasons​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#NonAttendanceReason -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#NonAttendanceReason">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalReason"/>
        <terms:description>A reason for a student’s failure to attend required sessions or activities, which may impact funding eligibility or compliance.</terms:description>
        <rdfs:label>NonAttendanceReason</rdfs:label>
        <skos:example>Absences due to illness, family emergencies, or personal issues.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#NonFundedStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#NonFundedStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingStatus"/>
        <terms:description>Represents a student or program that does not qualify for ESFA funding due to ineligibility or non-compliance.</terms:description>
        <rdfs:label>NonFundedStatus</rdfs:label>
        <skos:example>A program exceeding the funding cap or students not meeting residency requirements.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#NonQualificationAim -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#NonQualificationAim">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <terms:description>Learning aims that do not lead directly to a formal qualification but support personal or professional development.</terms:description>
        <rdfs:label>NonQualificationAim</rdfs:label>
        <skos:example>Enrichment activities, work placements, or employability skills sessions that are part of the study program​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#NotCompletedStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#NotCompletedStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#CompletionStatus"/>
        <terms:description>Status indicating that the student did not complete their study program, potentially impacting funding claims.</terms:description>
        <rdfs:label>NotCompletedStatus</rdfs:label>
        <skos:example>A student withdrawing midway due to personal or attendance-related issues.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Obligation -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Obligation">
        <terms:description>Represents a duty or requirement placed on individuals or institutions to comply with rules, funding conditions, or reporting requirements as outlined in the ESFA guidance.</terms:description>
        <rdfs:label>Obligation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#OpenLearningProvision -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#OpenLearningProvision">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningMode"/>
        <terms:description>Learning delivered in an open format, allowing flexibility in attendance and participation.</terms:description>
        <rdfs:label>OpenLearningProvision</rdfs:label>
        <skos:example>Self-paced online courses​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#PartTimeProgram -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#PartTimeProgram">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
        <terms:description>A study program with fewer than 540 planned hours, designed for part-time learners.</terms:description>
        <rdfs:label>PartTimeProgram</rdfs:label>
        <skos:example>Part-time vocational qualifications delivered to adult learners​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#PartTimeStudent -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#PartTimeStudent">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
        <owl:disjointWith>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#FullTimeStudent"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
                </owl:unionOf>
            </owl:Class>
        </owl:disjointWith>
        <terms:description>A student enrolled in a study program with fewer than 540 planned hours over an academic year, qualifying as part-time under ESFA funding rules.</terms:description>
        <rdfs:label>PartTimeStudent</rdfs:label>
        <skos:example>n adult learner attending evening classes for a vocational qualification.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Participant -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Participant">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Person"/>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <terms:description>An individual engaged in an educational or training program, including apprentices, trainees, or young people.</terms:description>
        <rdfs:label>Participant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ParticipationStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ParticipationStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <terms:description>Reflects the engagement level of a student in their study program or activities.</terms:description>
        <rdfs:label>ParticipationStatus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Person -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Person">
        <owl:equivalentClass rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <terms:description>Represents any individual involved in educational activities, including students, staff, and participants in funded programs.</terms:description>
        <rdfs:label>Person</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#PersonalReason -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#PersonalReason">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalReason"/>
        <terms:description>A reason related to personal circumstances, such as health, family responsibilities, or financial challenges, which may lead to non-attendance, withdrawal, or progression changes.</terms:description>
        <rdfs:label>PersonalReason</rdfs:label>
        <skos:example>Withdrawal from a study program due to personal health issues​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#PlannedHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#PlannedHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAspect"/>
        <terms:description>The total hours planned for the delivery of a study program, including qualification and non-qualification hours.</terms:description>
        <rdfs:label>PlannedHours</rdfs:label>
        <skos:example>Timetabled hours documented for ESFA funding compliance.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#PlannedHoursRecord -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#PlannedHoursRecord">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#RecordDocument"/>
        <terms:description>A document detailing the planned learning hours for a student, including both qualification and non-qualification hours. This record ensures alignment with ESFA&apos;s funding conditions and planned hours requirements.</terms:description>
        <rdfs:label>PlannedHoursRecord</rdfs:label>
        <skos:example>Timetables or schedules used to record and verify planned hours submitted in data return</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgrammeAim -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgrammeAim">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <rdfs:label>Programme Aim</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgrammeCompletionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgrammeCompletionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#CompletionActivity"/>
        <terms:description>Activities marking the official completion of a study program or learning aim.</terms:description>
        <rdfs:label>ProgrammeCompletionActivity</rdfs:label>
        <skos:example>Finalizing the records for students who meet all qualification criteria​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgrammeStart -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgrammeStart">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentActivity"/>
        <terms:description>The activity marking the initiation of a specific study program for a student.</terms:description>
        <rdfs:label>ProgrammeStart</rdfs:label>
        <skos:example>Official start dates recorded for funding compliance.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgrammeStartDate -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgrammeStartDate">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePoint"/>
        <terms:description>The official start date of a study program, marking the beginning of planned hours and eligibility for funding.</terms:description>
        <rdfs:label>ProgrammeStartDate</rdfs:label>
        <skos:example>Program start dates must match the information provided in ILR returns to avoid discrepancies during audits​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgressReport -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgressReport">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ReportDocument"/>
        <terms:description>A document detailing a student’s academic progress, attendance, and completion status for specific study programs. These reports may be used for funding audits, compliance checks, or student support.</terms:description>
        <rdfs:label>ProgressReport</rdfs:label>
        <skos:example>Reports showing students’ achievement in meeting their learning aims and progression to higher-level study or employment.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgressionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgressionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AssessmentActivity"/>
        <terms:description>Activities aimed at tracking or enabling a student’s academic or professional progression.</terms:description>
        <rdfs:label>ProgressionActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgressionReason -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgressionReason">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalReason"/>
        <terms:description>A reason explaining a student&apos;s progression within or beyond their current study program.</terms:description>
        <rdfs:label>ProgressionReason</rdfs:label>
        <skos:example>Progression to a higher qualification, an apprenticeship, or sustainable employment​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgressionStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgressionStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Status"/>
        <terms:description>Reflects the progression state of a student within or beyond their current study program.</terms:description>
        <rdfs:label>ProgressionStatus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ProgressionType -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ProgressionType">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionActivity"/>
        <terms:description xml:lang="en">Categorizes different types of educational and career progression pathways for students.</terms:description>
        <rdfs:label>ProgressionType</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Qualification -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Qualification">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>
        <terms:description>Refers to formal certifications awarded upon completion of a recognized learning aim.</terms:description>
        <rdfs:label>Qualification</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#QualificationAim -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#QualificationAim">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <terms:description>Learning aims that lead to a recognized qualification, such as academic or vocational certifications.</terms:description>
        <rdfs:label>QualificationAim</rdfs:label>
        <skos:example>A Level, vocational qualifications, or apprenticeship standards included in the ESFA’s list of eligible qualifications.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#QualifyingAttendanceActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#QualifyingAttendanceActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AttendanceActivity"/>
        <terms:description>Attendance activities that meet the criteria for funding eligibility and compliance, such as qualifying hours or required participation levels.</terms:description>
        <rdfs:label>QualifyingAttendanceActivity</rdfs:label>
        <skos:example>A student must attend six weeks of sessions to qualify for funding.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#QualifyingPeriod -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#QualifyingPeriod">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePeriod"/>
        <terms:description>A minimum duration a student must attend to qualify for funding. Often, this refers to the six-week attendance rule.</terms:description>
        <rdfs:label>QualifyingPeriod</rdfs:label>
        <skos:example>A student must attend their program for at least six weeks to qualify for funding eligibility​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#QualityAssuranceRecord -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#QualityAssuranceRecord">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ComplianceDocument"/>
        <terms:description>Records that provide evidence of adherence to quality standards in delivering study programs, attendance monitoring, and institutional processes. These documents demonstrate efforts to meet educational objectives and maintain service quality.</terms:description>
        <rdfs:label>QualityAssuranceRecord</rdfs:label>
        <skos:example>Records ensuring accurate data in ILR returns or evidence of robust attendance tracking systems.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ReEnrollmentActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ReEnrollmentActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentActivity"/>
        <rdfs:label>ReEnrollment Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Reason -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Reason">
        <terms:description>Represents the justification or cause for an action, event, or decision related to education and training, such as withdrawal, non-attendance, or progression.</terms:description>
        <rdfs:label>Reason</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RecordDocument -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RecordDocument">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Document"/>
        <terms:description>A category of documents used to log and maintain detailed information for compliance, attendance, and funding-related activities.</terms:description>
        <rdfs:label>RecordDocument</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RecordFundingValue -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RecordFundingValue">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#RecordingActivity"/>
        <terms:description>An activity that involves documenting the funding value associated with a specific student or program.</terms:description>
        <rdfs:label>RecordFundingValue</rdfs:label>
        <skos:example>Recording the funding rate in relation to planned hours.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RecordedPlannedHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RecordedPlannedHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#PlannedHours"/>
        <terms:description>The official record of planned hours submitted in funding claims or ILR returns.</terms:description>
        <rdfs:label>RecordedPlannedHours</rdfs:label>
        <skos:example>Hours documented for audit purposes​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RecordingActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RecordingActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <terms:description>Covers the documentation and recording of data required for compliance, attendance, and funding purposes.</terms:description>
        <rdfs:label>RecordingActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RecordingPlannedHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RecordingPlannedHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#RecordingActivity"/>
        <terms:description>The activity of recording the planned study or attendance hours for a program, as required for compliance.</terms:description>
        <rdfs:label>RecordingPlannedHours</rdfs:label>
        <skos:example>Timetables recorded at the start of the academic year</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RegulatoryBody -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RegulatoryBody">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Authority"/>
        <terms:description>A body responsible for creating, implementing, and monitoring compliance with regulations related to education and funding.</terms:description>
        <rdfs:label>RegulatoryBody</rdfs:label>
        <skos:example>The ESFA, which enforces funding rules and monitors compliance through audits and guidelines, is a regulatory body​</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ReplacementActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ReplacementActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <terms:description>Activities undertaken to replace a withdrawn or discontinued learning aim.</terms:description>
        <rdfs:label>ReplacementActivity</rdfs:label>
        <skos:example>A student switches to a similar qualification after withdrawing from their original learning aim​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ReportDocument -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ReportDocument">
        <rdfs:subClassOf rdf:resource="http://purl.org/cerif/frapo/Output"/>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Document"/>
        <terms:description>A document summarizing key institutional activities, compliance metrics, or funding-related outcomes. These reports are typically submitted to or reviewed by the ESFA.</terms:description>
        <rdfs:label>ReportDocument</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#ReportingObligation -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#ReportingObligation">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Obligation"/>
        <terms:description>A requirement to submit accurate and timely data to the ESFA, including Individualized Learner Records (ILR) and evidence of compliance.</terms:description>
        <rdfs:label>ReportingObligation</rdfs:label>
        <skos:example>Submission of ILR data to report attendance, withdrawals, and funding claims for audit purposes​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RetainedForFundingStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RetainedForFundingStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingStatus"/>
        <terms:description>Indicates that the student’s funding status is retained due to their compliance with attendance and program criteria.</terms:description>
        <rdfs:label>RetainedForFundingStatus</rdfs:label>
        <skos:example>Retention factors applied for students completing their planned hours.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RetainedStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RetainedStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ParticipationStatus"/>
        <terms:description>Represents that the student remains retained within the study program and meets retention criteria for funding.</terms:description>
        <rdfs:label>RetainedStatus</rdfs:label>
        <skos:example>Students continuing to the next term after meeting engagement thresholds​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#RetentionFactor -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#RetentionFactor">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingComponent"/>
        <terms:description>A multiplier applied to funding calculations to account for student retention. Institutions are incentivized to retain students until the completion of their study programs.</terms:description>
        <rdfs:label>RetentionFactor</rdfs:label>
        <skos:example>Retention factors are used to adjust funding based on whether students complete the full planned program.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SameInstitution -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SameInstitution">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Institution"/>
        <terms:description>Refers to the scenario where a student remains enrolled in the same educational institution for the entire duration of their program.</terms:description>
        <rdfs:label>SameInstitution</rdfs:label>
        <skos:example>Continuity of a student’s enrollment in a single institution ensures alignment with funding guidelines without duplication​</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SixWeekPeriod -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SixWeekPeriod">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePeriod"/>
        <terms:description>A specific six-week duration used to measure qualifying attendance or compliance with funding rules.</terms:description>
        <rdfs:label>SixWeekPeriod</rdfs:label>
        <skos:example>Institutions must confirm attendance records for students retained during the first six weeks of their program​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SixWeeksEarlyCompletion -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SixWeeksEarlyCompletion">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeCondition"/>
        <terms:description>Indicates early completion of a program within six weeks of the planned end date. Funding adjustments may apply in such cases.</terms:description>
        <rdfs:label>SixWeeksEarlyCompletion</rdfs:label>
        <skos:example>A student completing all their assessments six weeks before the scheduled end date of their program​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SkillsProgressionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SkillsProgressionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionActivity"/>
        <terms:description>Activities aimed at developing specific skills to improve employability or academic readiness.</terms:description>
        <rdfs:label>SkillsProgressionActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Staff -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Staff">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Person"/>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <terms:description>Represents individuals employed by an institution to support educational delivery, compliance, and administration.</terms:description>
        <rdfs:label>Staff</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Status -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Status">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#CompletionStatus"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#EnrollmentStatus"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#FundingStatus"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#ProgressionStatus"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <terms:description>Represents the state or condition of a student, program, or funding claim at a specific point in time, influencing eligibility, compliance, and progression.</terms:description>
        <rdfs:label>Status</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#StopFunding -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#StopFunding">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingImpact"/>
        <rdfs:label>Stop Funding</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Student -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Student">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#FullTimeStudent"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#PartTimeStudent"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawnStudent"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Person"/>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <terms:description>Represents an individual enrolled in a study program or training course at an educational institution.</terms:description>
        <rdfs:label>Student</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#StudentAttendance -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#StudentAttendance">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AttendanceActivity"/>
        <terms:description>An overarching activity encompassing the tracking and management of all attendance-related actions for students.</terms:description>
        <rdfs:label>StudentAttendance</rdfs:label>
        <skos:example>Comprehensive attendance tracking for ILR submissions.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#StudyProgramme -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#StudyProgramme">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#Programme"/>
        <terms:description>A structured educational program combining qualification and non-qualification aims, designed to meet the needs of students and prepare them for progression to further education, training, or employment.</terms:description>
        <rdfs:label>StudyProgramme</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SuccessfulCompletionActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SuccessfulCompletionActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#CompletionActivity"/>
        <terms:description>Activities indicating that a student has fully met all requirements of a program or qualification aim.</terms:description>
        <rdfs:label>SuccessfulCompletionActivity</rdfs:label>
        <skos:example>A completed qualification aim logged in the ILR.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SuccessfulCompletionStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SuccessfulCompletionStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AchievementStatus"/>
        <terms:description>Represents the successful fulfillment of all learning aims, including qualification and non-qualification components.</terms:description>
        <rdfs:label>SuccessfulCompletionStatus</rdfs:label>
        <skos:example>Achieving an A Level qualification with full attendance and assessments completed successfully.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SuccessfulProgression -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SuccessfulProgression">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionStatus"/>
        <terms:description>Indicates that the student has successfully progressed to a higher-level qualification, apprenticeship, or employment.</terms:description>
        <rdfs:label>SuccessfulProgression</rdfs:label>
        <skos:example>A student completing their study program and enrolling in a Level 3 apprenticeship​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SummerTerm -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SummerTerm">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePeriod"/>
        <terms:description>The term occurring during the summer months, which may include additional or supplemental learning activities.</terms:description>
        <rdfs:label>SummerTerm</rdfs:label>
        <skos:example>Summer term activities, such as exam preparation or catch-up sessions, may be included in planned hours.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SupportActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SupportActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AdministrativeActivity"/>
        <terms:description xml:lang="en">Activities designed to provide operational or administrative support for students, staff, and institutional processes.</terms:description>
        <rdfs:label>SupportActivity</rdfs:label>
        <skos:example xml:lang="en">Assistance in managing compliance documentation or helping students address attendance-related issues.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SustainableEmploymentProgression -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SustainableEmploymentProgression">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionType"/>
        <terms:description xml:lang="en">Represents a student&apos;s transition from education to long-term employment.</terms:description>
        <rdfs:label>SustainableEmploymentProgression</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#SustainableEmploymentStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#SustainableEmploymentStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#AchievementStatus"/>
        <terms:description>Indicates that a student has progressed into sustained employment upon completing their study program.</terms:description>
        <rdfs:label>SustainableEmploymentStatus</rdfs:label>
        <skos:example>A graduate securing full-time employment after completing a traineeship program​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TeachingStaff -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TeachingStaff">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Staff"/>
        <terms:description>Staff directly responsible for delivering educational content and supporting students in achieving their learning aims.</terms:description>
        <rdfs:label>TeachingStaff</rdfs:label>
        <skos:example>A tutor delivering guided learning sessions for A Level students.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TelephoneContact -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TelephoneContact">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#GeneralAssistanceActivity"/>
        <terms:description>Activities involving contacting students or guardians via phone to confirm attendance or address absences.</terms:description>
        <rdfs:label>TelephoneContact</rdfs:label>
        <skos:example>Follow-ups for non-attendance compliance.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TimeCondition -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TimeCondition">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
        <terms:description>Defines time-related rules or criteria that must be met to maintain funding compliance or eligibility.</terms:description>
        <rdfs:label>TimeCondition</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TimePeriod -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TimePeriod">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
        <terms:description>Represents a defined duration associated with educational or funding activities, compliance, or tracking.</terms:description>
        <rdfs:label>TimePeriod</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TimePoint -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TimePoint">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeRelated"/>
        <terms:description>Represents specific moments or dates within an academic or funding year that are critical for compliance, reporting, or funding calculations.</terms:description>
        <rdfs:label>TimePoint</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TimeRelated -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TimeRelated">
        <terms:description>Represents concepts related to time in the context of funding compliance, attendance tracking, and program delivery. This includes time-based conditions, periods, and specific points relevant to funding and student progress.</terms:description>
        <rdfs:label>TimeRelated</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TimetabledHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TimetabledHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAspect"/>
        <terms:description>Hours scheduled for the delivery of learning activities, including both qualification and non-qualification aims.</terms:description>
        <rdfs:label>TimetabledHours</rdfs:label>
        <skos:example>Weekly timetables showing planned sessions for students​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TotalHoursForYear -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TotalHoursForYear">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAspect"/>
        <terms:description>The sum of all planned hours for a student’s study program over an academic year.</terms:description>
        <rdfs:label>TotalHoursForYear</rdfs:label>
        <skos:example>540 planned hours for full-time 16-19 study programs.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TraditionalProvision -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TraditionalProvision">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningMode"/>
        <terms:description>Learning delivered through traditional in-person teaching methods.</terms:description>
        <rdfs:label>TraditionalProvision</rdfs:label>
        <skos:example>Lectures and classroom-based teaching.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Trainee -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Trainee">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Participant"/>
        <terms:description>A participant enrolled in a traineeship program designed to develop employability skills and prepare them for employment or an apprenticeship.</terms:description>
        <rdfs:label>Trainee</rdfs:label>
        <skos:example>A young person in a 6-month traineeship program with work placement and training components​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#Traineeship -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#Traineeship">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#StudyProgramme"/>
        <terms:description>A study program designed to prepare students for employment or further education, combining work placements with skills training.</terms:description>
        <rdfs:label>Traineeship</rdfs:label>
        <skos:example>A 6-month traineeship for students aged 16-24 focusing on employability skills​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TraineeshipFunding -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TraineeshipFunding">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingType"/>
        <terms:description>Funding specifically allocated for traineeships, which focus on preparing students for employment or apprenticeships by developing skills and providing work experience.</terms:description>
        <rdfs:label>TraineeshipFunding</rdfs:label>
        <skos:example>Traineeship funding based on planned hours and eligible activities​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TrainingProvider -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TrainingProvider">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EducationProvider"/>
        <terms:description>An organization delivering vocational training, skills development, or workforce preparation programs. Training providers may receive ESFA funding for delivering specific qualifications or employability skills.</terms:description>
        <rdfs:label>TrainingProvider</rdfs:label>
        <skos:example>Training providers offering technical qualifications approved on the ESFA list of funded qualifications.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TransferActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TransferActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentActivity"/>
        <terms:description>An enrollment activity involving the transfer of a student from one study program or institution to another.</terms:description>
        <rdfs:label>TransferActivity</rdfs:label>
        <skos:example>Updating ILR records when a student switches courses​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TransferredStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TransferredStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentStatus"/>
        <terms:description>Indicates that a student has transferred to another study program or institution.</terms:description>
        <rdfs:label>TransferredStatus</rdfs:label>
        <skos:example>A student transferring from one provider to another during the program​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#TwoWeeksEarlyCompletion -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#TwoWeeksEarlyCompletion">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimeCondition"/>
        <terms:description>Refers to a program completed two weeks earlier than planned. While less common than six-week adjustments, funding may still need recalibration.</terms:description>
        <rdfs:label>TwoWeeksEarlyCompletion</rdfs:label>
        <skos:example>A student achieving their qualification two weeks ahead of the planned completion date​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#UpdatePlannedHours -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#UpdatePlannedHours">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#RecordingActivity"/>
        <terms:description>An activity involving updates to the recorded planned hours, reflecting changes in the program structure or student participation.</terms:description>
        <rdfs:label>UpdatePlannedHours</rdfs:label>
        <skos:example>Updating planned hours after a reduction in a student’s timetable​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#VocationalQualification -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#VocationalQualification">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Qualification"/>
        <terms:description>Qualifications focused on practical skills and workplace readiness.</terms:description>
        <rdfs:label>VocationalQualification</rdfs:label>
        <skos:example>BTEC or NVQ qualifications included in ESFA funding eligibility​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#VocationalTrainingProgression -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#VocationalTrainingProgression">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#ProgressionType"/>
        <terms:description xml:lang="en">Covers transitions into alternative training programmes, such as skill-based certifications, employer-sponsored training, or workforce development initiatives.</terms:description>
        <rdfs:label>VocationalTrainingProgression</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WholeYear -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WholeYear">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#TimePeriod"/>
        <terms:description>Represents the entire academic or funding year as a single time period.</terms:description>
        <rdfs:label>WholeYear</rdfs:label>
        <skos:example>Institutions must report total planned hours and attendance for the whole year in ILR submissions​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Activity"/>
        <terms:description>Activities associated with the process of withdrawing a student from a program or institution.</terms:description>
        <rdfs:label>WithdrawalActivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalAdjustment -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalAdjustment">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#FundingAdjustment"/>
        <terms:description>An adjustment made when a student withdraws from their program. Funding is recalculated based on the student&apos;s attendance and engagement prior to withdrawal.</terms:description>
        <rdfs:label>WithdrawalAdjustment</rdfs:label>
        <skos:example>Partial funding claims made for students who withdraw early, based on the eligible learning complete.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalDateActivity -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalDateActivity">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalActivity"/>
        <terms:description>Recording the specific date of a student’s withdrawal from a program or institution.</terms:description>
        <rdfs:label>WithdrawalDateActivity</rdfs:label>
        <skos:example>Used to calculate funding adjustments.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalFromLearningAim -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalFromLearningAim">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalActivity"/>
        <terms:description>An activity where a student withdraws from a specific learning aim or course within a study program.</terms:description>
        <rdfs:label>WithdrawalFromLearningAim</rdfs:label>
        <skos:example>A student exits a single course while continuing other studies​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalFromStudyProgramme -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalFromStudyProgramme">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalActivity"/>
        <terms:description>An activity involving the withdrawal of a student from an entire study program.</terms:description>
        <rdfs:label>WithdrawalFromStudyProgramme</rdfs:label>
        <skos:example>Transitioning from a study program to a different one within the same institution​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalFromWholeProgramme -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalFromWholeProgramme">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalActivity"/>
        <terms:description>An activity marking the withdrawal of a student from all programs and courses at the institution.</terms:description>
        <rdfs:label>WithdrawalFromWholeProgramme</rdfs:label>
        <skos:example>A complete exit from education recorded in the ILR​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalProcess -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalProcess">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawalActivity"/>
        <terms:description>The administrative and procedural activities involved in managing and recording a student&apos;s withdrawal.</terms:description>
        <rdfs:label>WithdrawalProcess</rdfs:label>
        <skos:example>Completing and submitting withdrawal forms as evidence for audits.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalReason -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalReason">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#NonAttendanceReason"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#PersonalReason"/>
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#ProgressionReason"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Reason"/>
        <terms:description>A specific reason provided for a student withdrawing from a study program or qualification.</terms:description>
        <rdfs:label>WithdrawalReason</rdfs:label>
        <skos:example>Withdrawal due to employment opportunities, health issues, or transferring to another institution.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawalRecord -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawalRecord">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#RecordDocument"/>
        <terms:description>A document recording the details of a student&apos;s withdrawal from a study program, including the date, reason, and evidence of proper process adherence.</terms:description>
        <rdfs:label>WithdrawalRecord</rdfs:label>
        <skos:example>Withdrawal forms or records submitted as part of the ILR or school census data returns.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawnQualification -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawnQualification">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#LearningAim"/>
        <terms:description>A learning aim or qualification that a student has formally withdrawn from before completion.</terms:description>
        <rdfs:label>WithdrawnQualification</rdfs:label>
        <skos:example>Withdrawal due to personal reasons or a change in the study program, with funding adjustments made accordingly​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawnStatus -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawnStatus">
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#EnrollmentStatus"/>
        <terms:description>Status indicating that a student has formally withdrawn from their study program.</terms:description>
        <rdfs:label>WithdrawnStatus</rdfs:label>
        <skos:example>A withdrawn student is subject to funding adjustments based on their attendance before withdrawal​.</skos:example>
    </owl:Class>
    


    <!-- http://www.w3id.org/EFRO/EFRO#WithdrawnStudent -->


    <owl:Class rdf:about="http://www.w3id.org/EFRO/EFRO#WithdrawnStudent">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/EFRO/EFRO#Student"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3id.org/EFRO/EFRO#hasStatus"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3id.org/EFRO/EFRO#WithdrawnStatus"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/EFRO/EFRO#Student"/>
