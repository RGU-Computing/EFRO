                 rdfs:label "hasAimReference"@en .


###  http://www.w3id.org/efro/efro#hasAllocatedAmount
:hasAllocatedAmount rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :hasFundingValue ;
                    rdfs:domain :FundingElement ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:onDatatype xsd:decimal ;
                                 owl:withRestrictions ( [ xsd:minInclusive 0.00
                                                        ]
                                                      )
                               ] ;
                    <http://purl.org/dc/terms/description> "Represents the initially assigned funding amount for a student or programme before any modifications​."@en ;
                    rdfs:label "hasAllocatedAmount" .


###  http://www.w3id.org/efro/efro#hasAttendanceStatus
:hasAttendanceStatus rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :hasStatusValue ;
                     rdfs:domain :AttendanceStatus ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "Absent" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "Authorized" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "Present" ;
                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                               rdf:first "Unauthorized" ;
                                                                               rdf:rest rdf:nil
                                                                             ]
                                                                  ]
                                                       ]
                                            ]
                                ] ;
                     <http://purl.org/dc/terms/description> "Indicates whether a student is actively attending, absent, or has irregular attendance, affecting funding eligibility."@en ;
                     rdfs:label "hasAttendanceStatus" .


###  http://www.w3id.org/efro/efro#hasCompleteStatus
:hasCompleteStatus rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasStatusValue ;
                   rdfs:domain :CompletionStatus ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
