@prefix : <http://www.w3id.org/efro/efro#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3id.org/efro/efro#> .

<http://www.w3id.org/efro/efro> rdf:type owl:Ontology ;
                                 owl:imports <http://purl.org/cerif/frapo/> ,
                                             <http://purl.org/vocab/aiiso/schema#> ,
                                             <http://purl.org/vocab/vann/> ,
                                             <http://www.w3.org/2003/06/sw-vocab-status/ns> ,
                                             <http://xmlns.com/foaf/0.1/> ;
                                 <http://creativecommons.org/ns#license> <https://creativecommons.org/licenses/by/4.0/> ;
                                 <http://purl.org/dc/elements/1.1/contributor> "Ikechukwu Nkisi-Orji" ;
                                 <http://purl.org/dc/elements/1.1/coverage> "Educational domain including student management, program administration, funding allocation, and assessment tracking." ;
                                 <http://purl.org/dc/elements/1.1/rights> "This ontology is licensed under the Creative Commons Attribution 4.0 International License (CC-BY 4.0). You are free to share and adapt this ontology, provided that you give appropriate credit. More details: https://creativecommons.org/licenses/by/4.0/" ;
                                 <http://purl.org/dc/terms/contributor> "David Corsar" ;
                                 <http://purl.org/dc/terms/creator> "Umair Arshad" ;
                                 <http://purl.org/dc/terms/description> "An ontology for modeling educational institutions' funding, assessment, and student management systems. It captures the relationships between students, educational programs, funding mechanisms, and assessment processes. The ontology supports tracking student statuses, attendance, progression, and funding eligibility while maintaining compliance with educational standards and reporting requirements." ;
                                 <http://purl.org/dc/terms/format> "rdf/xml" ;
                                 <http://purl.org/dc/terms/language> "en" ;
                                 <http://purl.org/dc/terms/title> "Educational Funding Regulations Ontology (EFRO)" ;
                                 rdfs:comment """This ontology models:
    1. Student enrollment and status tracking
    2. Educational program structures and requirements
    3. Funding mechanisms and eligibility criteria
    4. Assessment and progression tracking
    5. Attendance monitoring systems
    6. Documentation and compliance requirements
    7. Administrative processes and support systems""" ;
                                 owl:versionInfo 1.0 .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
<http://creativecommons.org/ns#license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/coverage
<http://purl.org/dc/elements/1.1/coverage> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3id.org/efro/efro#belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           owl:inverseOf :includes ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Learning
                                       :Person
                                     )
                       ] ;
           <http://purl.org/dc/terms/description> "Indicates an entity's relationship to a parent entity, such as a student belonging to an institution or a funding scheme."@en ;
           rdfs:label "belongsTo" .


###  http://www.w3id.org/efro/efro#constrains
:constrains rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :TimeRelated ;
            rdfs:range :Activity .


###  http://www.w3id.org/efro/efro#correspondsToLearningAim
:correspondsToLearningAim rdf:type owl:ObjectProperty ;
                          rdfs:domain :ClassRegister ;
                          rdfs:range :LearningAim ;
                          rdfs:label "correspondsToLearningAim" .


###  http://www.w3id.org/efro/efro#defines
:defines rdf:type owl:ObjectProperty ;
         rdfs:domain :FundingElement ;
         rdfs:range :FundingImpact ;
         rdfs:label "defines" .


###  http://www.w3id.org/efro/efro#enrolledIn
:enrolledIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasEnrollee ;
            rdfs:domain :Student ;
            rdfs:range :StudyProgramme ;
            <http://purl.org/dc/terms/description> "Represents a student's enrolment in a specific programme, institution, or funding category​."@en ;
            rdfs:label "enrolledIn" .


###  http://www.w3id.org/efro/efro#evaluates
:evaluates rdf:type owl:ObjectProperty ;
           rdfs:domain :FundingElement ;
           rdfs:range :Activity .


###  http://www.w3id.org/efro/efro#forProgramme
:forProgramme rdf:type owl:ObjectProperty ;
              rdfs:domain :Status ;
              rdfs:range :StudyProgramme ;
              rdfs:comment "for programme" .


###  http://www.w3id.org/efro/efro#fulfills
:fulfills rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :Evidence ;
          rdfs:range :Obligation ;
          rdfs:label "fulfills" .


###  http://www.w3id.org/efro/efro#givernedBy
:givernedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :FundingRegulations ;
            rdfs:range :FundingElement ;
            rdfs:label "giverned By" .


###  http://www.w3id.org/efro/efro#hasAchievementStatus
:hasAchievementStatus rdf:type owl:ObjectProperty ;
                      rdfs:domain :Student ;
                      rdfs:range :AchievementStatus ;
                      <http://purl.org/dc/terms/description> "Represents the status of a student’s academic or vocational achievement, tracking progress and outcomes."@en ;
                      rdfs:label "hasAchievementStatus" .


###  http://www.w3id.org/efro/efro#hasActiveParticipation
:hasActiveParticipation rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasParticipationStatus ;
                        rdfs:domain :Student ;
                        rdfs:range :ActiveParticipationStatus ;
                        <http://purl.org/dc/terms/description> "Indicates that a student is actively engaged in their learning and meets attendance requirements."@en ;
                        rdfs:label "hasActiveParticipation" .


###  http://www.w3id.org/efro/efro#hasActualAttendance
:hasActualAttendance rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasAttendanceRecord ;
                     rdfs:domain :Student ;
                     rdfs:range :ActualAttendanceActivity ;
                     <http://purl.org/dc/terms/description> "Tracks the actual attendance hours recorded for a student, ensuring compliance with funding regulations​."@en ;
                     rdfs:label "hasActualAttendance" .


###  http://www.w3id.org/efro/efro#hasAdminSystemAccount
:hasAdminSystemAccount rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasOnlineAccount ;
                       rdfs:domain :Staff ;
                       rdfs:range <http://xmlns.com/foaf/0.1/OnlineAccount> ;
                       rdfs:label "hasAdminSystemAccount" .


###  http://www.w3id.org/efro/efro#hasAimStatus
:hasAimStatus rdf:type owl:ObjectProperty ;
              rdfs:domain :LearningAim ;
              rdfs:range :Status ;
              rdfs:label "has aim status" .


###  http://www.w3id.org/efro/efro#hasApplicationStatus
:hasApplicationStatus rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasStatus ;
                      rdfs:domain <http://purl.org/cerif/frapo/Application> ;
                      rdfs:range :Status ;
                      rdfs:label "hasApplicationStatus" .


###  http://www.w3id.org/efro/efro#hasApplicationType
:hasApplicationType rdf:type owl:ObjectProperty ;
                    rdfs:domain <http://purl.org/cerif/frapo/Application> ;
                    rdfs:range :LearningAim ;
                    rdfs:label "hasApplicationType" .


###  http://www.w3id.org/efro/efro#hasApprenticeshipFunding
:hasApprenticeshipFunding rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasFundingElement ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( :Learning
                                                      :Student
                                                    )
                                      ] ;
                          rdfs:range :ApprenticeshipFunding ;
                          <http://purl.org/dc/terms/description> "Denotes funding specifically allocated for apprenticeship programmes, covering training, assessment, and employer incentives."@en ;
                          rdfs:label "hasApprenticeshipFunding" .


###  http://www.w3id.org/efro/efro#hasAssessmentRecord
:hasAssessmentRecord rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isAssessmentRecord ;
                     rdfs:domain :Student ;
                     rdfs:range :AssessmentActivity ;
                     <http://purl.org/dc/terms/description> "Links a student’s assessment records, such as examinations, assignments, and overall performance tracking."@en ;
                     rdfs:label "hasAssessmentRecord" .


###  http://www.w3id.org/efro/efro#hasAssignmentCompletion
:hasAssignmentCompletion rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :hasAssessmentRecord ;
                         rdfs:domain :Student ;
                         rdfs:range :AssignmentActivity ;
                         <http://purl.org/dc/terms/description> "Indicates that a student has submitted and completed a required assignment as part of their programme​."@en ;
                         rdfs:label "hasAssignmentCompletion" .


###  http://www.w3id.org/efro/efro#hasAssociatedDocument
:hasAssociatedDocument rdf:type owl:ObjectProperty ;
                       rdfs:domain :Obligation ;
                       rdfs:range :Document ;
                       <http://purl.org/dc/terms/description> "Links an entity (e.g., student, programme, funding application) to a supporting document such as a learning agreement, enrolment form, or funding claim."@en ;
                       rdfs:label "hasAssociatedDocument" .


###  http://www.w3id.org/efro/efro#hasAttendanceEvidence
:hasAttendanceEvidence rdf:type owl:ObjectProperty ;
                       rdfs:domain :LastAttendanceActivity ;
                       rdfs:range :ClassRegister ;
                       <http://purl.org/dc/terms/description> "Refers to records that demonstrate a student's attendance in learning activities, which is essential for funding eligibility."@en ;
                       rdfs:label "hasAttendanceEvidence" .


###  http://www.w3id.org/efro/efro#hasAttendanceRecord
:hasAttendanceRecord rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isAttendanceRecordOf ;
                     rdfs:domain :Student ;
                     rdfs:range :AttendanceActivity ;
                     <http://purl.org/dc/terms/description> "Represents a student’s attendance data, used to validate funding claims and student engagement."@en ;
                     rdfs:label "hasAttendanceRecord" .


###  http://www.w3id.org/efro/efro#hasClassRegister
:hasClassRegister rdf:type owl:ObjectProperty ;
                  rdfs:domain :LastAttendanceActivity ;
                  rdfs:range :ClassRegister ;
                  <http://purl.org/dc/terms/description> "Links a student or programme to an official attendance register used to record participation​."@en ;
                  rdfs:label "hasClassRegister" .


###  http://www.w3id.org/efro/efro#hasCompletionStatus
:hasCompletionStatus rdf:type owl:ObjectProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :CompletionStatus
                                                 :Student
                                               )
                                 ] ;
                     rdfs:range :CompletionStatus ;
                     <http://purl.org/dc/terms/description> "Represents the completion status of a student's learning aim or study programme, whether successful, withdrawn, or incomplete."@en ;
                     rdfs:label "hasCompletionStatus" .


###  http://www.w3id.org/efro/efro#hasComplianceRequirement
:hasComplianceRequirement rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasLegalDuty ;
                          owl:inverseOf :isComplianceRequirementFor ;
                          rdfs:domain :FundedInstitution ;
                          rdfs:range :ComplianceObligation ;
                          <http://purl.org/dc/terms/description> "Defines the legal and regulatory requirements institutions must meet to receive and maintain funding."@en ;
                          rdfs:label "hasComplianceRequirement" .


###  http://www.w3id.org/efro/efro#hasDistanceLearningProvision
:hasDistanceLearningProvision rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :hasLearningMode ;
                              rdfs:domain :Learning ;
                              rdfs:range :DistanceLearningProvision ;
                              <http://purl.org/dc/terms/description> "Represents learning that occurs remotely, without requiring physical attendance at an institution."@en ;
                              rdfs:label "hasDistanceLearningProvision" .


###  http://www.w3id.org/efro/efro#hasDocumentation
:hasDocumentation rdf:type owl:ObjectProperty ;
                  rdfs:domain :Reason ;
                  rdfs:range :Document ;
                  <http://purl.org/dc/terms/description> "Indicates the presence of required supporting documents for enrolment, funding applications, or compliance."@en ;
                  rdfs:label "hasDocumentation" .


###  http://www.w3id.org/efro/efro#hasDoubleFundingRestriction
:hasDoubleFundingRestriction rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasFundingCondition ;
                             rdfs:domain :FundingElement ;
                             rdfs:range :DoubleFundingRestriction ;
                             <http://purl.org/dc/terms/description> "Prevents a student from being funded simultaneously by multiple programmes or institutions."@en ;
                             rdfs:label "hasDoubleFundingRestriction" .


###  http://www.w3id.org/efro/efro#hasESFAFunding
:hasESFAFunding rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasFundingElement ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Learning
                                            :Student
                                          )
                            ] ;
                rdfs:range :ESFAFunding ;
                <http://purl.org/dc/terms/description> "Indicates financial support provided by the Education and Skills Funding Agency (ESFA) for 16-19 education, adult learning, and traineeships."@en ;
                rdfs:label "hasESFAFunding" .


###  http://www.w3id.org/efro/efro#hasEarlyCompletion
:hasEarlyCompletion rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasAchievementStatus ;
                    rdfs:domain :AchievementStatus ;
                    rdfs:range :EarlyCompletionStatus ;
                    <http://purl.org/dc/terms/description> "Indicates that a student has completed their programme earlier than planned, which may impact funding adjustments​."@en ;
                    rdfs:label "hasEarlyCompletion" .


###  http://www.w3id.org/efro/efro#hasEarlyCompletionStatus
:hasEarlyCompletionStatus rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasParticipationStatus ;
                          rdfs:domain :Student ;
                          rdfs:range :EarlyCompletionStatus ;
                          <http://purl.org/dc/terms/description> "Captures the status of a student who has completed their programme ahead of the planned schedule."@en ;
                          rdfs:label "hasEarlyCompletionStatus" .


###  http://www.w3id.org/efro/efro#hasEnrollee
:hasEnrollee rdf:type owl:ObjectProperty ;
             rdfs:domain :StudyProgramme ;
             rdfs:range :Student ;
             <http://purl.org/dc/terms/description> "Links a student to an educational institution or programme in which they are officially enrolled​."@en ;
             rdfs:label "hasEnrollee" .


###  http://www.w3id.org/efro/efro#hasExaminationResult
:hasExaminationResult rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasAssessmentRecord ;
                      rdfs:domain :Student ;
                      rdfs:range :ExaminationActivity ;
                      <http://purl.org/dc/terms/description> "Represents the student’s performance in formal exams, recorded for academic and funding purposes."@en ;
                      rdfs:label "hasExaminationResult" .


###  http://www.w3id.org/efro/efro#hasFundedStudent
:hasFundedStudent rdf:type owl:ObjectProperty ;
                  rdfs:domain :FundingClaimReport ;
                  rdfs:range :Student ;
                  <http://purl.org/dc/terms/description> "Connects a student to a funding allocation, indicating that they have been approved for financial support​."@en ;
                  rdfs:label "hasFundedStudent" .


###  http://www.w3id.org/efro/efro#hasFunding
:hasFunding rdf:type owl:ObjectProperty ;
            owl:inverseOf :isFundedBy ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :EducationalEntity
                                        :Learning
                                      )
                        ] ;
            rdfs:range :FundingElement ;
            <http://purl.org/dc/terms/description> "Represents the financial resources allocated to a student, programme, or institution."@en ;
            rdfs:label "hasFunding" .


###  http://www.w3id.org/efro/efro#hasFundingBand
:hasFundingBand rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasFundingElement ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Learning
                                            :Student
                                          )
                            ] ;
                rdfs:range :FundingBand ;
                <http://purl.org/dc/terms/description> "Represents the predefined levels or categories that determine funding allocation for specific learning aims."@en ;
                rdfs:label "hasFundingBand" .


###  http://www.w3id.org/efro/efro#hasFundingCondition
:hasFundingCondition rdf:type owl:ObjectProperty ;
                     rdfs:domain :FundingElement ;
                     rdfs:range :FundingCondition ;
                     <http://purl.org/dc/terms/description> "Defines the rules and conditions that govern funding eligibility, allocation, and restrictions."@en ;
                     rdfs:label "hasFundingCondition" .


###  http://www.w3id.org/efro/efro#hasFundingElement
:hasFundingElement rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Learning
                                               :Student
                                             )
                               ] ;
                   rdfs:range :FundingElement ;
                   <http://purl.org/dc/terms/description> "Represents different components of funding allocated to a student, institution, or programme."@en ;
                   rdfs:label "hasFundingElement" .


###  http://www.w3id.org/efro/efro#hasFundingImpact
:hasFundingImpact rdf:type owl:ObjectProperty ;
                  rdfs:domain :Activity ;
                  rdfs:range :FundingImpact ;
                  rdfs:label "has funding impact" .


###  http://www.w3id.org/efro/efro#hasFundingObligation
:hasFundingObligation rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasLegalDuty ;
                      owl:inverseOf :isFundingObligationFor ;
                      rdfs:domain :FundedInstitution ;
                      rdfs:range :FundingObligation ;
                      <http://purl.org/dc/terms/description> "Specifies the conditions that institutions must fulfill to qualify for and continue receiving financial support."@en ;
                      rdfs:label "hasFundingObligation" .


###  http://www.w3id.org/efro/efro#hasFundingOverlap
:hasFundingOverlap rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasFundingCondition ;
                   rdfs:domain :FundingElement ;
                   rdfs:range :FundingOverlap ;
                   <http://purl.org/dc/terms/description> "Identifies cases where a student’s funding periods overlap, ensuring they do not receive duplicate funding."@en ;
                   rdfs:label "hasFundingOverlap" .


###  http://www.w3id.org/efro/efro#hasFundingStartStatus
:hasFundingStartStatus rdf:type owl:ObjectProperty ;
                       rdfs:domain :Student ;
                       rdfs:range :FundingStartStatus ;
                       <http://purl.org/dc/terms/description> "Captures the initial status of a student or programme at the beginning of a funding period, ensuring eligibility and compliance​."@en ;
                       rdfs:label "hasFundingStartStatus" .


###  http://www.w3id.org/efro/efro#hasGroupStatus
:hasGroupStatus rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasStatus ;
                rdfs:domain <http://xmlns.com/foaf/0.1/Group> ;
                rdfs:range :Status ;
                rdfs:label "hasGroupStatus" .


###  http://www.w3id.org/efro/efro#hasGroupType
:hasGroupType rdf:type owl:ObjectProperty ;
              rdfs:domain <http://xmlns.com/foaf/0.1/Group> ;
              rdfs:range :LearningMode ;
              rdfs:label "hasGroupType" .


###  http://www.w3id.org/efro/efro#hasGuidedLearning
:hasGuidedLearning rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasLearningMode ;
                   rdfs:domain :Learning ;
                   rdfs:range :GuidedLearning ;
                   <http://purl.org/dc/terms/description> "Indicates structured learning activities under the direct supervision of an instructor or tutor."@en ;
                   rdfs:label "hasGuidedLearning" .


###  http://www.w3id.org/efro/efro#hasInfrastructureType
:hasInfrastructureType rdf:type owl:ObjectProperty ;
                       rdfs:domain <http://purl.org/cerif/frapo/InfrastructureEntity> ;
                       rdfs:range :LearningMode ;
                       rdfs:label "hasInfrastructureType" .


###  http://www.w3id.org/efro/efro#hasInitialPlannedHours
:hasInitialPlannedHours rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasPlannedHoursManagement ;
                        rdfs:domain :Learning ;
                        rdfs:range :InitialPlannedHours ;
                        <http://purl.org/dc/terms/description> "Captures the number of learning hours assigned to a student when they first enroll in a study programme."@en ;
                        rdfs:label "hasInitialPlannedHours" .


###  http://www.w3id.org/efro/efro#hasInstitutionalAccount
:hasInstitutionalAccount rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :hasOnlineAccount ;
                         rdfs:domain :EducationalEntity ;
                         rdfs:range <http://xmlns.com/foaf/0.1/OnlineAccount> ;
                         rdfs:label "hasInstitutionalAccount" .


###  http://www.w3id.org/efro/efro#hasLastAttendance
:hasLastAttendance rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasAttendanceRecord ;
                   rdfs:domain :Student ;
                   rdfs:range :LastAttendanceActivity ;
                   <http://purl.org/dc/terms/description> "Captures the most recent attendance record before a student withdraws or completes their programme​."@en ;
                   rdfs:label "hasLastAttendance" .


###  http://www.w3id.org/efro/efro#hasLearningAim
:hasLearningAim rdf:type owl:ObjectProperty ;
                rdfs:domain :StudyProgramme ;
                rdfs:range :LearningAim ;
                <http://purl.org/dc/terms/description> "Defines the objectives or intended outcomes of a student’s learning programme."@en ;
                rdfs:label "hasLearningAim" .


###  http://www.w3id.org/efro/efro#hasLearningMode
:hasLearningMode rdf:type owl:ObjectProperty ;
                 rdfs:domain :Learning ;
                 rdfs:range :LearningMode ;
                 <http://purl.org/dc/terms/description> "Specifies the method or format in which learning is delivered."@en ;
                 rdfs:label "hasLearningMode" .


###  http://www.w3id.org/efro/efro#hasLearningPlatformAccount
:hasLearningPlatformAccount rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :hasOnlineAccount ;
                            rdfs:domain :Student ;
                            rdfs:range <http://xmlns.com/foaf/0.1/OnlineAccount> ;
                            rdfs:label "hasLearningPlatformAccount" .


###  http://www.w3id.org/efro/efro#hasLegalDuty
:hasLegalDuty rdf:type owl:ObjectProperty ;
              rdfs:domain :EducationalEntity ;
              rdfs:range :Obligation ;
              <http://purl.org/dc/terms/description> "Represents legal responsibilities institutions must fulfill regarding funding, student participation, and compliance."@en ;
              rdfs:label "hasLegalDuty" .


###  http://www.w3id.org/efro/efro#hasNonAttendance
:hasNonAttendance rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasAttendanceRecord ;
                  rdfs:domain :Student ;
                  rdfs:range :NonAttendanceActivity ;
                  <http://purl.org/dc/terms/description> "Records instances where a student has failed to attend required sessions, which may impact their funding eligibility."@en ;
                  rdfs:label "hasNonAttendance" .


###  http://www.w3id.org/efro/efro#hasNonQualificationAim
:hasNonQualificationAim rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasLearningAim ;
                        rdfs:domain :StudyProgramme ;
                        rdfs:range :NonQualificationAim ;
                        <http://purl.org/dc/terms/description> "Refers to learning activities that do not lead to formal qualifications, such as employability training or enrichment activities."@en ;
                        rdfs:label "hasNonQualificationAim" .


###  http://www.w3id.org/efro/efro#hasOnlineAccount
:hasOnlineAccount rdf:type owl:ObjectProperty ;
                  rdfs:domain :Person ;
                  rdfs:range <http://xmlns.com/foaf/0.1/OnlineAccount> ;
                  rdfs:label "hasOnlineAccount" .


###  http://www.w3id.org/efro/efro#hasOpenLearningProvision
:hasOpenLearningProvision rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasLearningMode ;
                          rdfs:domain :Learning ;
                          rdfs:range :OpenLearningProvision ;
                          <http://purl.org/dc/terms/description> "Refers to flexible learning models that allow students to access materials and progress at their own pace."@en ;
                          rdfs:label "hasOpenLearningProvision" .


###  http://www.w3id.org/efro/efro#hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :participatesIn ;
                rdfs:domain :Activity ;
                rdfs:range :Person ;
                <http://purl.org/dc/terms/description> "Identifies students or staff involved in a study programme, funding scheme, or learning activity."@en ;
                rdfs:label "hasParticipant" .


###  http://www.w3id.org/efro/efro#hasParticipationStatus
:hasParticipationStatus rdf:type owl:ObjectProperty ;
                        rdfs:domain :Student ;
                        rdfs:range :ParticipationStatus ;
                        <http://purl.org/dc/terms/description> "Represents a student's engagement level in their learning programme."@en ;
                        rdfs:label "hasParticipationStatus" .


