                                            rdf:first "Complete" ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first "EarlyCompletion" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "Incomplete" ;
                                                                  rdf:rest rdf:nil
                                                                ]
                                                     ]
                                          ]
                              ] ;
                   <http://purl.org/dc/terms/description> "Specifies whether a student has completed their learning aim successfully, failed, or left the course before completion​."@en ;
                   rdfs:label "hasCompleteStatus" .


###  http://www.w3id.org/efro/efro#hasCompletionDate
:hasCompletionDate rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasTemporalValue ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :CompletionActivity
                                               :Learning
                                             )
                               ] ;
                   rdfs:range xsd:dateTime ;
                   <http://purl.org/dc/terms/description> "Specifies the official date when a student successfully completes a learning aim or study programme​."@en ;
                   rdfs:label "hasCompletionDate" .


###  http://www.w3id.org/efro/efro#hasCompletionDetails
:hasCompletionDetails rdf:type owl:DatatypeProperty ;
